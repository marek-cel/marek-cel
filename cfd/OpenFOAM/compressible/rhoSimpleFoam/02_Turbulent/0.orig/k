FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "include/boundaryConditions"

// ************************************************************************* //

dimensions [ 0 2 -2 0 0 0 0 ];

internalField uniform $turbulentKE;


boundaryField
{
    inlet
    {
        type        fixedValue;
        value       $internalField;
    }

    outlet
    {
        type        inletOutlet;
        value       $internalField;
        inletValue  $internalField;
    }
    
    freestream
    {
        type        slip;
    }

    model
    {
        type        kqRWallFunction;
        value       $internalField;
    }
    
    "proc.*"
    {
        type        processor;
    }
}


