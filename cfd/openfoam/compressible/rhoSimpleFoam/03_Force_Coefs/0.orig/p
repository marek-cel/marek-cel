FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}

// ************************************************************************** //

#include "include/boundaryConditions"

// ************************************************************************** //

dimensions [ 1 -1 -2 0 0 0 0 ];

// ************************************************************************** //

internalField uniform $pressure;

// ************************************************************************** //

boundaryField
{
    inlet
    {
        type        zeroGradient;
    }

    outlet
    {
        type        fixedValue;
        value       $internalField;
    }

    freestream
    {
        type        zeroGradient;
    }
    
    model
    {
        type        zeroGradient;
    }
    
    "proc.*"
    {
        type        processor;
    }
}

